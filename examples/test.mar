extern { fn print(val: int) fn print_str(val: str) }


impl Range {
    fn __next__(self): Option<int> {
        if self.min < self.max {
            self.min += 1
            some(self.min-1)
        } else {
            none()
        }

    }
}


fn print_opt<T>(opt: Option<T>) {
    match opt {
        some => print_str("yay"),
        none => print_str("nay"),
    }
}


@startup
fn main() {
    print_opt(none::<int>())
}
 
