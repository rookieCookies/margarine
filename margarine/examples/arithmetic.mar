@startup
system fn main() {
    let x = 1
    let y = 2

    let add = x + y
    let sub = x - y
    let mul = x * y
    let div = x / y
    let rem = x % y
    let shift_l = x << y 
    let shift_r = x >> y
    let and = x & y
    let or = x | y
    let xor = x ^ y


    let mut z = 0

    z += add
    z -= sub 
    z *= mul 
    z /= div 
    z %= rem
}
